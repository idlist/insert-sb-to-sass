{
  "name": "insert-sb-to-sass",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vitest",
    "dev:site": "vite --config vite.site.config.ts",
    "build": "run-p build:lib build:site",
    "build:lib": "vite build && npm run build:type",
    "build:type": "tsc --project tsconfig.prod.json",
    "build:site": "vue-tsc -b && vite build --config vite.site.config.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "npm-run-all2": "^7.0.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "sass": "^1.80.6",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.8"
  }
}
